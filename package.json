{
  "name": "fortuna-lottery",
  "version": "5.0.0",
  "description": "Fortuna Lottery - Telegram Lottery MiniApp с непрерывными тиражами",
  "main": "server/start-with-migrate.js",
  "scripts": {
    "start": "node server/start-with-migrate.js",
    "dev": "nodemon server/start-with-migrate.js",
    "worker": "node server/cron-worker.js",
    "start:all": "concurrently \"npm start\" \"npm run worker\"",
    "dev:all": "concurrently \"npm run dev\" \"npm run worker\"",
    "migrate": "node server/init-db.js",
    "test-db": "node server/test-db-query.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "pg": "^8.11.3",
    "crypto": "^1.0.1",
    "node-cron": "^3.0.2",
    "concurrently": "^8.2.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "lottery",
    "telegram",
    "miniapp",
    "gaming",
    "crypto"
  ]
}
